{
	"extends": ["@oly_op", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"env": {
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"max-len": "off",
		"no-restricted-exports": "off",
		"@typescript-eslint/require-await": "off",
		"prefer-arrow/prefer-arrow-functions": "off"
	}
}